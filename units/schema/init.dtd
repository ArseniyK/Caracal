<!ELEMENT document (module*,user*)>

<!ELEMENT module EMPTY>
<!ATTLIST module
	name CDATA #REQUIRED
	order CDATA #IMPLIED
	preload (yes|no) "no"
>

<!ELEMENT user EMPTY>
<!ATTLIST user
	username CDATA #REQUIRED
	password CDATA #REQUIRED
	fullname CDATA #REQUIRED
	level CDATA #REQUIRED
>

