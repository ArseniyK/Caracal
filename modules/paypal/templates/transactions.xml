<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE document SYSTEM "../../../units/schema/template.dtd">
<document>
	<!-- Header -->
	<div class="scrollable_list">
		<div class="list_header">
			<span class="column" style="width: 200px;">
				<_text constant="column_payer" module="paypal"/>
			</span>
			<span class="column" style="width: 100px;">
				<_text constant="column_date" module="paypal"/>
			</span>
			<span class="column" style="width: 70px;">
				<_text constant="column_gross" module="paypal"/>
			</span>
			<span class="column">
				<_text constant="column_type" module="paypal"/>
			</span>
		</div>
		<div class="list_content" style="height: 250px">
			<!-- List Content -->
			<_transaction_list/>
		</div>
	</div>
</document>
