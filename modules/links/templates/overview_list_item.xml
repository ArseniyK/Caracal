<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE document SYSTEM "../../../units/schema/template.dtd">
<document>
	<div class="list_item">
		<span class="column" style="width: 300px;">
			<cms:var name="$params['text']"/>
		</span>
		<span class="column" style="width: 100px;">
			<div class="progress_bar" style="width: 90px;">
				<div class="gauge" style="'width: '.$params['display_percent'].'%'" cms:eval="style"/>
			</div>
		</span>
		<span class="column" style="width: 70px;">
			<cms:var name="$params['sponsored_clicks']"/>
			<cms:raw text="/"/>
			<cms:var name="$params['display_limit']"/>
		</span>
		<span class="column" style="width: 70px;">
			<cms:var name="$params['item_change']"/>
		</span>
	</div>
</document>
