<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE document SYSTEM "../../../units/schema/template.dtd">
<document>
	<div class="list_item">
		<span class="column" style="width: 200px;">
			<_var name="$params['fullname']"/>
		</span>
		<span class="column" style="width: 150px;">
			<_var name="$params['username']"/>
		</span>
		<span class="column" style="width: 50px; text-align: center;">
			<_var name="$params['level']"/>
		</span>
		<span class="column">
			<_var name="$params['item_change']"/><_raw text=" "/>
			<_var name="$params['item_delete']"/>
		</span>
	</div>
</document>
