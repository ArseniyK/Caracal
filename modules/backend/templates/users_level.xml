<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE document SYSTEM "../../../units/schema/template.dtd">
<document>
	<_if condition="$params['level'] == $params['selected']">
		<option value="$params['level']" eval="value" selected="selected"><_var name="$params['level']"/></option>
	</_if>
	<_if condition="$params['level'] != $params['selected']">
		<option value="$params['level']" eval="value"><_var name="$params['level']"/></option>
	</_if>
</document>
