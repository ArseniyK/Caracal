<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE document SYSTEM "../../../units/schema/template.dtd">
<document>
	<cms:if condition="$params['selected'] == $params['version']">
		<option value="$params['version']" selected="selected" eval="value"><cms:var name="$params['version']"/></option>
	</cms:if>
	<cms:if condition="$params['selected'] != $params['version']">
		<option value="$params['version']" eval="value"><cms:var name="$params['version']"/></option>
	</cms:if>
</document>
