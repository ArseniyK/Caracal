<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE document SYSTEM "../../../units/schema/template.dtd">
<document>
	<div class="list_item">
		<span class="column" style="width: 250px;">
			<_var name="$params['title'][$language]"/>
		</span>
		<span class="column" style="width: 200px;">
			<_var name="$params['group_title'][$language]"/>
		</span>
		<span class="column" style="width: 70px; text-align: center;">
			<_var name="$params['active_char']"/>
		</span>
		<span class="column">
			<_var name="$params['item_change']"/><_raw text=" "/>
			<_var name="$params['item_delete']"/>
		</span>
	</div>
</document>
