<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE document SYSTEM "../../../units/schema/template.dtd">
<document>
	<div class="main_menu">
		<_var name="$params['link_new']"/>
	</div>

	<div class="scrollable_list">
		<div class="list_header">
			<span class="column" style="width: 100px;">
				<_text constant="column_id" module="youtube"/>
			</span>
			<span class="column" style="width: 300px">
				<_text constant="column_title" module="youtube"/>
			</span>
			<span class="column">
				<_text constant="column_options"/>
			</span>
		</div>
		<div class="list_content" style="height: 250px">
			<_video_list/>
		</div>
	</div>

</document>