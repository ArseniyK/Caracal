<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE document SYSTEM "../../../units/schema/template.dtd">
<document>
	<div class="area">
		<span><cms:text constant="label_score" module="page_speed"/></span>
		<h1><cms:var name="$params['score']"/></h1>
		<span><cms:text constant="label_number_problems" module="page_speed"/></span>
		<h2><cms:var name="$params['number_problems']"/></h2>
		<span><cms:text constant="label_number_hosts" module="page_speed"/></span>
		<h2><cms:var name="$params['number_hosts']"/></h2>
	</div>

	<div class="area">
		<span><cms:text constant="label_title" module="page_speed"/></span>
		<h2><cms:var name="$params['title']"/></h2>
		<span><cms:text constant="label_number_resources" module="page_speed"/></span>
		<h3><cms:var name="$params['number_resources']"/></h3>
		<span><cms:text constant="label_number_static" module="page_speed"/></span>
		<h3><cms:var name="$params['number_static']"/></h3>
		<span><cms:text constant="label_number_css" module="page_speed"/></span>
		<h3><cms:var name="$params['number_css']"/></h3>
		<span><cms:text constant="label_number_javascript" module="page_speed"/></span>
		<h3><cms:var name="$params['number_javascript']"/></h3>
	</div>

	<div class="area">
		<img
			src="$params['image_score']"
			cms:eval="src"
		/>
	</div>

	<div class="area">
		<span><cms:text constant="label_bytes_request" module="page_speed"/></span>
		<h3><cms:var name="$params['bytes_request']"/></h3>
		<span><cms:text constant="label_bytes_html" module="page_speed"/></span>
		<h3><cms:var name="$params['bytes_html']"/></h3>
		<span><cms:text constant="label_bytes_image" module="page_speed"/></span>
		<h3><cms:var name="$params['bytes_image']"/></h3>
		<span><cms:text constant="label_bytes_css" module="page_speed"/></span>
		<h3><cms:var name="$params['bytes_css']"/></h3>
		<span><cms:text constant="label_bytes_javascript" module="page_speed"/></span>
		<h3><cms:var name="$params['bytes_javascript']"/></h3>
		<span><cms:text constant="label_bytes_other" module="page_speed"/></span>
		<h3><cms:var name="$params['bytes_other']"/></h3>
	</div>

	<div class="area double">
		<img
			src="$params['image_bytes']"
			cms:eval="src"
		/>
	</div>
</document>
