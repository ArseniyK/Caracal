<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE document SYSTEM "../../../units/schema/template.dtd">
<document>
	<!-- Window Menu -->
	<div class="main_menu">
		<cms:var name="$params['check_page_speed']"/>
		<cms:var name="$params['set_api_key']"/>
	</div>

	<div id="page_speed_notebook">
		<!-- General Information -->
		<div class="general">
			<_general_information/>
		</div>

		<!-- Detailed Information -->
		<div class="details">
			<_detailed_information/>
		</div>
	</div>

	<script type="text/javascript">
		var notebook = new Notebook('page_speed_notebook');
		notebook
			.setPageTitle(0, language_handler.getText('page_speed', 'tab_general'))
			.setPageTitle(1, language_handler.getText('page_speed', 'tab_details'));
	</script>
</document>
