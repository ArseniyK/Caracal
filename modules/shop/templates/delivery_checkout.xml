<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE document SYSTEM "../../../units/schema/template.dtd">
<document>
	<label>
		<input
			type="radio"
			name="delivery_method"
			value="$params['name']"
			cms:eval="value"
		/>
		<span>
			<img
				src="$params['small_image']"
				alt="$params['title']"
				cms:eval="src,alt"
				width="100"
				height="28"
			/>
		</span>
	</label>
</document>
