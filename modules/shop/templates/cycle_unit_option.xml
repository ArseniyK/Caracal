<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE document SYSTEM "../../../units/schema/template.dtd">
<document>
	<cms:if condition="$params['selected']">
		<option value="$params['id']" eval="value" selected="selected">
			<cms:var name="$params['text']"/>
		</option>
	</cms:if>
	<cms:if condition="!$params['selected']">
		<option value="$params['id']" eval="value">
			<cms:var name="$params['text']"/>
		</option>
	</cms:if>
</document>
