<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE document SYSTEM "../../../units/schema/template.dtd">
<document>
	<_if condition="$params['selected'] == $params['id']">
		<option value="$params['id']" cms:eval="value" selected="selected">
			<_var name="$params['name']"/>
		</option>
	</_if>
	<_if condition="$params['selected'] != $params['id']">
		<option value="$params['id']" cms:eval="value">
			<_var name="$params['name']"/>
		</option>
	</_if>
</document>
